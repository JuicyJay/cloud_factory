<?xml version="1.0"?>
<launch>
    <node pkg = "cloud_factory" type = "random_clouds" name = "random_clouds">
        <param name = "width" type = "double" value = "9.999"/>
        <param name = "height" type = "double" value = "9.999"/>
        <param name = "resolution" type = "double" value = "0.1"/>
        <param name = "mode" type = "int" value = "3"/>
        <param name = "gauss_A" type = "double" value = "-0.8"/>
        <param name = "gauss_center_x" type = "double" value = "5.0"/>
        <param name = "gauss_center_y" type = "double" value = "5.0"/>
        <param name = "gauss_sigma_x" type = "double" value = "10.0"/>
        <param name = "gauss_sigma_y" type = "double" value = "1.0"/>
        <param name = "gauss_power_x" type = "double" value = "9.99"/>
        <param name = "gauss_power_y" type = "double" value = "9.99"/>
    </node>    
</launch>